<script type="text/javascript">
$(document).ready(function () {
	$('a[id^="lattest_"]').each(function () {
		App.initializeTooltip($(this), true, 'warehouse/item_info_image');
	});
});	
</script>
<% 
if(typeof items != 'undefined'){ 
if(items != false){
%>
<% 
for(var i=0; i< items.length; i++) { 
%>
<li>

	<div class="latestforum">
		<h3><a id="lattest_<%= i+1 %>" href="<%= base_url %>market/buy/<%= items[i].id %>" data-info="<%= items[i].item %>"><%= items[i].name %></a></h3>
		<span><%= items[i].price %></span>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>
</li>
<% } %>
<%
}
else{
%>
	<div class="i_note"><%= App.lc.translate('No Items Found.').fetch() %></div>
<%
}
} 
%>