<% 
if(typeof masterlevel != 'undefined'){ 
if(masterlevel != false){
%>
<table class="ranking-table">
	<thead>
	<tr class="main-tr">
		<th style="text-align:center;">#</th>
		<th style="text-align:center;"><%= App.locale.name %></th>
		<th style="text-align:center;"><%= App.locale.mlevel %></th>
	</tr>
	</thead>
	<tbody>
	<% for(var i=0; i<masterlevel.length; i++) { %>
	<tr>
		<td style="text-align:center;"><%= i+1 %></td>
		<td><a href="<%= base_url %>character/<%= bin2hex(masterlevel[i].name) %>/<%= server_selected %>"><%= masterlevel[i].name %></a></td>
		<td><%= masterlevel[i].level %></td>
	</tr>  
	<% } %>
	</tbody>
</table>  
<% if(typeof cache_time != 'undefined'){ %>
<div style="text-align: right;padding-top: 15px;"><%= cache_time %></div>
<% } %>
<% 
}
else{
%>
<div style="padding: 0 30px 0px 50px;"><div class="i_note"><%= App.locale.no_players_found %></div></div>
<%
}
} 
%>