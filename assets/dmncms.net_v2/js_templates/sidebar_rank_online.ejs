<% 
if(typeof online != 'undefined'){ 
if(online != false){
%>
<% for(var i=0; i<online.length; i++) { %>
<li>
	<div class="characters">
		<span class="number"><%= i+1 %></span>
		<h3><a href="<%= base_url %>character/<%= bin2hex(online[i].name) %>/<%= server_selected %>"><%= online[i].name %></a> <% if(config.online.display_country == 1) { %><span class="f16"><span class="flag <%= online[i].country %>"></span></span><% } %></h3>
		<span><%= App.lc.translate('Online Time').fetch() %>: <%= online[i].h %> <%= App.lc.translate('H').fetch() %> <%= online[i].m %> <%= App.lc.translate('Min').fetch() %></span>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>
</li>
<% } %>
<% 
}
else{
%>
<div class="i_note"><%= App.lc.translate('No Players Found').fetch() %></div>
<%
}
} 
%>