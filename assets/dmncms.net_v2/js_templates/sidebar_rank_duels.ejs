<% 
if(typeof duels != 'undefined'){ 
if(duels != false){
%>
<% for(var i=0; i<duels.length; i++) { %>
<li>
	<div class="characters">
		<span class="number"><%= i+1 %></span>
		<h3><a href="<%= base_url %>character/<%= bin2hex(duels[i].name) %>/<%= server_selected %>"><%= duels[i].name %></a></h3>
		<span><%= App.lc.translate('Win').fetch() %>/<%= App.lc.translate('Lose').fetch() %>: <%= duels[i].win %>/<%= duels[i].lose %></span>
		<div class="clear"></div>
	</div>
	<div class="clear"></div>
</li>
<% } %>
<% 
}
else{
%>
<div class="i_note"><%= App.lc.translate('No Players Found').fetch() %></div>
<%
}
} 
%>